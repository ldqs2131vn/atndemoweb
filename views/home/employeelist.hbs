<title>Employee list</title>
<div class="table-responsive">
    <table class="table table-bordered">
      <thead>
        <tr>
          <th>Employee Name</th>
          <th>Gender</th>
            <th>City</th>
            <th>Mobile</th>
        </tr>
      </thead>
      <tbody>
        {{#each list}}
        <tr>
          <td>{{this.name}}</td>
          <td>{{this.gender}}</td>
            <td>{{this.city}}</td>
            <td>{{this.phone}}</td>
        </tr>
        {{/each}}
      </tbody>
    </table>
  </div>